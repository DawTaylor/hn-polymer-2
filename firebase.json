{
  "hosting": {
    "public": "build/es5-bundled",
    "rewrites": [
      {
        "source": "/hn/**", "function": "hn"
      },
      {
        "source": "!/__/**",
        "destination": "/index.html"
      },
      {
        "source": "**/!(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/",
        "headers": [{"key": "Link", "value": "</bower_components/webcomponentsjs/custom-elements-es5-adapter.js>;rel=preload;as=script,</bower_components/webcomponentsjs/webcomponents-loader.js>;rel=preload;as=script,</src/hn-app.html>;rel=preload;as=document,</hn/news?page=1>;rel=preload"}]
      },
      {
        "source": "**/(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
        "headers": [{"key": "Cache-Control", "value": "public, max-age=36000, s-maxage=600, stale-while-revalidate=3600"}]
      }
    ]
  }
}
